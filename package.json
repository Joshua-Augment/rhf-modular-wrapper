{
  "name": "rhf-modular-wrapper",
  "version": "1.0.0",
  "description": "Modular Wrapper for React Hook Form using context, plug and play smart components style",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "start-storybook -p 6006",
    "build": "rm -rf /dist && npm run build:esm && npm run build:cjs && npm run copy-esm && npm run copy-cjs",
    "build:esm": "tsc",
    "build:cjs": "tsc --module CommonJS --outDir dist/cjs",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "copy-esm": "copyfiles -V -E -u 1 ./src/components/styling/*.css ./dist/esm",
    "copy-cjs": "copyfiles -V -E -u 1 ./src/components/styling/*.css ./dist/cjs"
  },
  "keywords": [
    "React-Hook-Form",
    "React"
  ],
  "author": "joshuajs",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@storybook/addon-actions": "^6.5.16",
    "@storybook/addon-essentials": "^6.4.22",
    "@storybook/addon-interactions": "^6.5.16",
    "@storybook/addon-links": "^6.5.16",
    "@storybook/builder-webpack4": "^6.5.16",
    "@storybook/manager-webpack4": "^6.5.16",
    "@storybook/react": "^6.1.21",
    "@storybook/testing-library": "^0.0.13",
    "@types/node": "^18.13.0",
    "@types/react": "^18.0.28",
    "@types/react-color": "^3.0.6",
    "@types/react-datepicker": "^4.8.0",
    "@types/react-dom": "^18.0.10",
    "babel-loader": "^8.3.0",
    "copyfiles": "^2.4.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^4.9.5"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@lexical/react": "^0.8.0",
    "@lexical/utils": "^0.8.0",
    "@mui/icons-material": "^5.11.0",
    "@mui/material": "^5.11.8",
    "@types/quill": "^2.0.10",
    "i": "^0.3.7",
    "lexical": "^0.8.0",
    "npm": "^9.4.2",
    "react-color": "^2.19.3",
    "react-datepicker": "^4.10.0",
    "react-hook-form": "^7.43.1",
    "react-icons": "^4.7.1",
    "styled-components": "^5.3.6"
  }
}
