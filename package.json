{
  "name": "rhf-modular-wrapper",
  "version": "3.1.1",
  "description": "Modular Wrapper for React Hook Form using context, plug and play smart components style",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Joshua-Augment/rhf-modular-wrapper"
  },
  "scripts": {
    "start": "storybook dev -p 6006",
    "build": "npm run build:esm && npm run build:cjs && npm run copy-esm && npm run copy-cjs",
    "build-patch": "npm run build:esm && npm run build:cjs && npm run copy-esm && npm run copy-cjs && npm version patch && npm publish",
    "build-minor": "npm run build:esm && npm run build:cjs && npm run copy-esm && npm run copy-cjs && npm version minor && npm publish",
    "build-major": "npm run build:esm && npm run build:cjs && npm run copy-esm && npm run copy-cjs && npm version major && npm publish",
    "build:esm": "tsc",
    "build:cjs": "tsc --module CommonJS --outDir dist/cjs",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "copy-esm": "copyfiles -V -E -u 1 ./src/components/styling/*.css ./dist/esm",
    "copy-cjs": "copyfiles -V -E -u 1 ./src/components/styling/*.css ./dist/cjs"
  },
  "keywords": [
    "React-Hook-Form",
    "React"
  ],
  "author": "joshuajs",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/preset-env": "^7.22.10",
    "@babel/preset-react": "^7.22.5",
    "@babel/preset-typescript": "^7.22.5",
    "@storybook/addon-actions": "^8.0.0",
    "@storybook/addon-essentials": "^8.0.0",
    "@storybook/addon-interactions": "^8.0.0",
    "@storybook/addon-links": "^8.0.0",
    "@storybook/addon-mdx-gfm": "^8.0.0",
    "@storybook/addon-onboarding": "^8.0.0",
    "@storybook/addon-webpack5-compiler-babel": "^3.0.3",
    "@storybook/blocks": "^8.0.0",
    "@storybook/react": "^8.0.0",
    "@storybook/react-vite": "^8.0.0",
    "@storybook/react-webpack5": "^8.0.0",
    "@types/node": "^18.13.0",
    "@types/react": "^18.0.28",
    "@types/react-color": "^3.0.6",
    "@types/react-datepicker": "^4.8.0",
    "@types/react-dom": "^18.0.10",
    "babel-loader": "^8.3.0",
    "copyfiles": "^2.4.1",
    "fix-tsc-es-imports": "^0.1.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "storybook": "^8.0.0",
    "storybook-addon-performance": "^0.17.3",
    "typescript": "^4.9.5"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@hookform/resolvers": "^2.9.11",
    "@storybook/test": "^8.0.0",
    "@types/quill": "^2.0.10",
    "@uppy/core": "^3.0.6",
    "@uppy/dashboard": "^3.2.2",
    "@uppy/drag-drop": "^3.0.1",
    "@uppy/file-input": "^3.0.1",
    "@uppy/progress-bar": "^3.0.1",
    "@uppy/react": "^3.1.0",
    "@uppy/xhr-upload": "^3.1.0",
    "array-move": "^4.0.0",
    "file-loader": "^6.2.0",
    "html-react-parser": "^4.0.0",
    "i": "^0.3.7",
    "npm": "^9.4.2",
    "quill": "^1.3.7",
    "react-color": "^2.19.3",
    "react-datepicker": "^4.10.0",
    "react-dropzone": "^14.2.3",
    "react-hook-form": "^7.50.1",
    "react-icons": "^4.7.1",
    "react-quill": "^2.0.0",
    "react-radio-buttons": "^1.2.2",
    "react-range-slider-input": "^3.0.7",
    "react-select": "^5.7.0",
    "react-switch": "^7.0.0",
    "styled-components": "^5.3.6",
    "yup": "^0.32.11"
  }
}
