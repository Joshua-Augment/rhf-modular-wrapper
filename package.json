{
  "name": "rhf-modular-wrapper",
  "version": "1.8.0",
  "description": "Modular Wrapper for React Hook Form using context, plug and play smart components style",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Joshua-Augment/rhf-modular-wrapper"
  },
  "scripts": {
    "start": "start-storybook -p 6006",
    "build": "rm -rf /dist && npm run build:esm && npm run build:cjs && npm run copy-esm && npm run copy-cjs",
    "build-patch": "rm -rf /dist && npm run build:esm && npm run build:cjs && npm run copy-esm && npm run copy-cjs && npm version patch && npm publish",
    "build-minor": "rm -rf /dist && npm run build:esm && npm run build:cjs && npm run copy-esm && npm run copy-cjs && npm version minor && npm publish",
    "build-major": "rm -rf /dist && npm run build:esm && npm run build:cjs && npm run copy-esm && npm run copy-cjs && npm version major && npm publish",
    "build:esm": "tsc",
    "build:cjs": "tsc --module CommonJS --outDir dist/cjs",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "copy-esm": "copyfiles -V -E -u 1 ./src/components/styling/*.css ./dist/esm",
    "copy-cjs": "copyfiles -V -E -u 1 ./src/components/styling/*.css ./dist/cjs"
  },
  "keywords": [
    "React-Hook-Form",
    "React"
  ],
  "author": "joshuajs",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@storybook/addon-actions": "^6.5.16",
    "@storybook/addon-essentials": "^6.4.22",
    "@storybook/addon-interactions": "^6.5.16",
    "@storybook/addon-links": "^6.5.16",
    "@storybook/builder-webpack4": "^6.5.16",
    "@storybook/manager-webpack4": "^6.5.16",
    "@storybook/react": "^6.1.21",
    "@storybook/testing-library": "^0.0.13",
    "@types/node": "^18.13.0",
    "@types/react": "^18.0.28",
    "@types/react-color": "^3.0.6",
    "@types/react-datepicker": "^4.8.0",
    "@types/react-dom": "^18.0.10",
    "babel-loader": "^8.3.0",
    "copyfiles": "^2.4.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^4.9.5"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "dependencies": {
    "@cyntler/react-doc-viewer": "^1.12.0",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@hookform/resolvers": "^2.9.11",
    "@mui/icons-material": "^5.11.0",
    "@mui/material": "^5.11.8",
    "@types/quill": "^2.0.10",
    "@types/react-pdf": "^6.2.0",
    "@uppy/core": "^3.0.6",
    "@uppy/dashboard": "^3.2.2",
    "@uppy/drag-drop": "^3.0.1",
    "@uppy/file-input": "^3.0.1",
    "@uppy/progress-bar": "^3.0.1",
    "@uppy/react": "^3.1.0",
    "@uppy/xhr-upload": "^3.1.0",
    "array-move": "^4.0.0",
    "file-loader": "^6.2.0",
    "i": "^0.3.7",
    "npm": "^9.4.2",
    "pdfjs-dist": "^3.4.120",
    "quill": "^1.3.7",
    "react-color": "^2.19.3",
    "react-datepicker": "^4.10.0",
    "react-dropzone": "^14.2.3",
    "react-hook-form": "^7.43.1",
    "react-icons": "^4.7.1",
    "react-pdf": "^5.3.2",
    "react-quill": "^2.0.0",
    "react-radio-buttons": "^1.2.2",
    "react-select": "^5.7.0",
    "styled-components": "^5.3.6",
    "yup": "^0.32.11"
  }
}
